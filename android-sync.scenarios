base_jvm_args = [
  "-Dorg.gradle.internal.operations.verbose.locking=true"
]

base_parameters {
  warm-ups = 2
  iterations = 5

  system-properties {
    "org.gradle.workers.max" = "8"
    "org.gradle.caching" = "true"
  }

  jvm-args = ${base_jvm_args}

  measured-build-ops = [
    # Operations such as 'Compile script build.gradle.kts (BODY)'
    { type = "org.gradle.internal.scripts.CompileScriptBuildOperationType", measurement-kind = "wall_clock_time" }
    # Operations such as 'Configure Build'
    { type = "org.gradle.initialization.ConfigureBuildBuildOperationType", measurement-kind = "time_to_last_inclusive" }
  ]
}

base_build_scenario = ${base_parameters} {
  tasks = ["help"]
  gradle-args = ["--dry-run"]
}

base_scenario = ${base_parameters} {
  android-studio-sync {
    studio-jvm-args = ["-Xms8g", "-Xmx8g"]
    idea-properties = [
      "gradle.tooling.models.parallel.fetch=true" // This does not enable parallel fetch, only allows it
    ]
  }
}

base_pre_build_cleaning = ${base_scenario} {
  // In-between builds, clear the Gradle User Home in a special way.
  // Remove everything except external dependencies to avoid downloading them again.
  // The external dependencies are populated by the initial warm-up builds.
  clear-dir = [{
    schedule = BUILD
    root = GRADLE_USER_HOME
    target = "."
    keep = [
      "caches"
      "wrapper" // keep the wrapper to avoid re-downloading Gradle, mimics `clear-gradle-user-home-before` option
      "kotlin-profile" // this directory contains only lightweight Kotlin stats, and Android Studio throws logs many exceptions, if it gets deleted while AS is running
    ]
  }, {
    schedule = BUILD
    root = GRADLE_USER_HOME
    target = "caches"
    keep = ["modules-2"]
  }, {
    schedule = BUILD
    target = ".gradle"
    keep = ["config.properties"]
  }]

  delete-file = [{
    schedule = BUILD
    target = "build-logic/.gradle"
  }, {
    schedule = BUILD
    target = "build"
  }]
}

#######################################################
# Gradle modes - sets of flags to control performance #
#######################################################

# Note: careful with array values, since they are not merged/concatenated, but replaced!
# That's why we use base_jvm_args variable to keep common args.

# CC: Vanilla Configuration Cache
mode_cc {
  system-properties {
    "org.gradle.configuration-cache" = "true"
    "org.gradle.configuration-cache.parallel" = "false"
    "org.gradle.parallel" = "false"
    "org.gradle.configureondemand" = "false"
    "org.gradle.unsafe.isolated-projects" = "false"
  }
}
# Parallel CC: Configuration Cache with Parallel Store
mode_pcc {
  system-properties {
    "org.gradle.configuration-cache" = "true"
    "org.gradle.configuration-cache.parallel" = "true"
    "org.gradle.parallel" = "false"
    "org.gradle.configureondemand" = "false"
    "org.gradle.unsafe.isolated-projects" = "false"
  }
}
# Parallel CC and Parallel Execution (--parallel)
mode_pcc_par {
  system-properties {
    "org.gradle.configuration-cache" = "true"
    "org.gradle.configuration-cache.parallel" = "true"
    "org.gradle.parallel" = "true"
    "org.gradle.configureondemand" = "false"
    "org.gradle.unsafe.isolated-projects" = "false"
  }
}
# Parallel CC and Parallel Execution (--parallel) and Configure-on-Demand
mode_pcc_par_cod {
  system-properties {
    "org.gradle.configuration-cache" = "true"
    "org.gradle.configuration-cache.parallel" = "true"
    "org.gradle.parallel" = "true"
    "org.gradle.configureondemand" = "true"
    "org.gradle.unsafe.isolated-projects" = "false"
  }
}
# Parallel IP: Isolated Projects with parallel project configuration
mode_ip {
  system-properties {
    "org.gradle.unsafe.isolated-projects" = "true"
  }
  jvm-args = ${base_jvm_args} [
    "-Dorg.gradle.internal.isolated-projects.parallel=true"
    "-Dorg.gradle.internal.isolated-projects.caching=false"
  ]
}
# Incremental IP: Parallel IP with incremental project configuration
mode_iip {
  system-properties {
    "org.gradle.unsafe.isolated-projects" = "true"
  }
  jvm-args = ${base_jvm_args} [
    "-Dorg.gradle.internal.isolated-projects.parallel=true"
    "-Dorg.gradle.internal.isolated-projects.caching=tooling"
  ]
}
mode_max_workers {
  system-properties {
    "org.gradle.workers.max" = "-1"
  }
}
mode_2_workers {
  system-properties {
    "org.gradle.workers.max" = "2"
  }
}

